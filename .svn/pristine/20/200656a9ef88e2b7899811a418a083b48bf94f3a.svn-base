package com.vilu.springboot.web.app.pojo;


import java.util.List;

public class ItemPiso {

		private Long id;

		private String nombre;

		private String calle;
		
		private String pisoAltura;

		private long tarifaBase;
		
		private String habitaciones;

		private String numero;

		private String poblacion;

		private Boolean activo;
		
		private String motivo;
		
		private String detalle;
		
		private int personas;

		private List<ItemFoto> itemfotos;
		
		private List<ItemTarifa> itemTarifas;
		
		private List<String> ocupacion;
		
		private String coordenadaX;
		
		private String coordenadaY;

		public Long getId() {
			return id;
		}

		public void setId(Long id) {
			this.id = id;
		}

		public String getNombre() {
			return nombre;
		}

		public void setNombre(String nombre) {
			this.nombre = nombre;
		}

		public String getCalle() {
			return calle;
		}

		public void setCalle(String calle) {
			this.calle = calle;
		}

		public String getPisoAltura() {
			return pisoAltura;
		}

		public void setPisoAltura(String pisoAltura) {
			this.pisoAltura = pisoAltura;
		}

		public long getTarifaBase() {
			return tarifaBase;
		}

		public void setTarifaBase(long tarifaBase) {
			this.tarifaBase = tarifaBase;
		}

		public String getHabitaciones() {
			return habitaciones;
		}

		public void setHabitaciones(String habitaciones) {
			this.habitaciones = habitaciones;
		}

		public String getNumero() {
			return numero;
		}

		public void setNumero(String numero) {
			this.numero = numero;
		}

		public String getPoblacion() {
			return poblacion;
		}

		public void setPoblacion(String poblacion) {
			this.poblacion = poblacion;
		}

		public Boolean getActivo() {
			return activo;
		}

		public void setActivo(Boolean activo) {
			this.activo = activo;
		}

		public String getMotivo() {
			return motivo;
		}

		public void setMotivo(String motivo) {
			this.motivo = motivo;
		}

		public String getDetalle() {
			return detalle;
		}

		public void setDetalle(String detalle) {
			this.detalle = detalle;
		}

		public int getPersonas() {
			return personas;
		}

		public void setPersonas(int personas) {
			this.personas = personas;
		}


		public List<ItemFoto> getItemfotos() {
			return itemfotos;
		}

		public void setItemfotos(List<ItemFoto> itemfotos) {
			this.itemfotos = itemfotos;
		}

	

		public List<ItemTarifa> getItemTarifas() {
			return itemTarifas;
		}

		public void setItemTarifas(List<ItemTarifa> itemTarifas) {
			this.itemTarifas = itemTarifas;
		}

		public List<String> getOcupacion() {
			return ocupacion;
		}

		public void setOcupacion(List<String> ocupacion) {
			this.ocupacion = ocupacion;
		}
		
		

		public String getCoordenadaX() {
			return coordenadaX;
		}

		public void setCoordenadaX(String coordenadaX) {
			this.coordenadaX = coordenadaX;
		}

		public String getCoordenadaY() {
			return coordenadaY;
		}

		public void setCoordenadaY(String coordenadaY) {
			this.coordenadaY = coordenadaY;
		}

		public ItemPiso() {
			super();
		}
		
		

		public ItemPiso(Long id, String nombre, String calle, String pisoAltura, long tarifaBase, String habitaciones,
				String numero, String poblacion, Boolean activo, String motivo, String detalle, int personas,
				List<ItemFoto> itemfotos, List<ItemTarifa> itemTarifas,List<String> ocupacion,String coordenadaX,String coordenadaY) {
			super();
			this.id = id;
			this.nombre = nombre;
			this.calle = calle;
			this.pisoAltura = pisoAltura;
			this.tarifaBase = tarifaBase;
			this.habitaciones = habitaciones;
			this.numero = numero;
			this.poblacion = poblacion;
			this.activo = activo;
			this.motivo = motivo;
			this.detalle = detalle;
			this.personas = personas;
			this.itemfotos = itemfotos;
			this.itemTarifas = itemTarifas;
			this.ocupacion=ocupacion;
			this.coordenadaX=coordenadaX;
			this.coordenadaY=coordenadaY;
		}

		
		
}
