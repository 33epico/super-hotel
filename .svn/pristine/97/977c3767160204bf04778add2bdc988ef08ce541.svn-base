package com.vilu.springboot.web.app;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.context.annotation.Configuration;
import org.springframework.context.annotation.PropertySource;

@Configuration
@PropertySource("classpath:./messages_en.properties")
public class MessagePropertiesEN {

	@Value("${error.reserva.nologing.en}")
	public static String errnologing;
	
	@Value("${error.reserva.nopisos.en}")
	public static  String errnopisos;
	
	@Value("${error.reserva.generico.en}")
	public static String errgenerico;
	
	@Value("${error.reserva.promo.en}")
	public static String errpromo;
	
	@Value("${error.reserva.novalida.en}")
	public static String errresernovalida;
	
	@Value("${peticion.reserva.cancelacion.en}")
	public static String peticionCancelacion;
	
	@Value("${text.mensajes.notificacion.en}")
	public  String notificacion;
	
	@Value("${peticion.reserva.mensaje.cancelacion.en}")
	public  String mensajeCancelacion;
	
	@Value("${text.listado.nopisos.en}")
	public  String nopisos;
	
	@Value("${text.piso.error}")
	public  String error;
	
	@Value("${error.usuario.repetido.en}")
	public String errUsuRepetido;

	public static String getErrnologing() {
		return errnologing;
	}

	public static String getErrnopisos() {
		return errnopisos;
	}

	public static String getErrgenerico() {
		return errgenerico;
	}

	public static String getErrpromo() {
		return errpromo;
	}

	public static String getErrresernovalida() {
		return errresernovalida;
	}

	public static String getPeticionCancelacion() {
		return peticionCancelacion;
	}

	public String getNotificacion() {
		return notificacion;
	}

	public String getMensajeCancelacion() {
		return mensajeCancelacion;
	}

	public String getNopisos() {
		return nopisos;
	}

	public String getError() {
		return error;
	}

	public String getErrUsuRepetido() {
		return errUsuRepetido;
	}

	
	
	
}


